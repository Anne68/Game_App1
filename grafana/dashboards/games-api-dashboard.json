{
  "dashboard": {
    "id": null,
    "uid": "games-api-ml",
    "title": "Games API ML Monitoring",
    "tags": ["ml", "api", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 30,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "type": "graph",
        "title": "Prediction Latency (P50, P95, P99)",
        "targets": [
          {
            "expr": "histogram_quantile(0.5, rate(prediction_latency_seconds_bucket[5m]))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(prediction_latency_seconds_bucket[5m]))",
            "legendFormat": "P95", 
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(prediction_latency_seconds_bucket[5m]))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "yAxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "type": "graph", 
        "title": "Model Predictions Rate",
        "targets": [
          {
            "expr": "rate(model_predictions_total[5m])",
            "legendFormat": "Predictions/sec",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8},
        "type": "stat",
        "title": "Model Accuracy",
        "targets": [
          {
            "expr": "model_accuracy_gauge",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {"calcs": ["lastNotNull"]},
          "graphMode": "area",
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.7},
                {"color": "green", "value": 0.8}
              ]
            },
            "max": 1,
            "min": 0
          }
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8},
        "type": "stat",
        "title": "Error Rate", 
        "targets": [
          {
            "expr": "sum(rate(model_predictions_total{status=\"error\"}[5m])) / sum(rate(model_predictions_total[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {"calcs": ["lastNotNull"]},
          "graphMode": "area", 
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.01},
                {"color": "red", "value": 0.05}
              ]
            },
            "max": 1,
            "min": 0
          }
        }
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8},
        "type": "gauge",
        "title": "Model Drift Score",
        "targets": [
          {
            "expr": "prediction_drift_gauge", 
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.15},
                {"color": "red", "value": 0.3}
              ]
            },
            "max": 1,
            "min": 0
          }
        }
      }
    ],
    "time": {
      "from": "now-1h", 
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    }
  }
}
